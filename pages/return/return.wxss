.return-container {
  height: 1334rpx;
  background-color: #f2f2f2;
}

/* 表格 */

.table-header {
  margin-top:20rpx;
  width: 100%;
  height: 60rpx;
  display: flex;
  font-size: 28rpx;
  background-color: #fff;
  line-height: 60rpx;
  /* border:2rpx solid blue; */
}

.table-header view {
 width:150rpx;
 text-align:center;
}

.table-body {
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  border-bottom: 2rpx solid #ccc;
}

.table-body text {
  /* border:1rpx solid red; */
  display: inline-block;
  width: 150rpx;
  text-align: center;
  font-size: 28rpx;
  box-sizing: border-box;
}
